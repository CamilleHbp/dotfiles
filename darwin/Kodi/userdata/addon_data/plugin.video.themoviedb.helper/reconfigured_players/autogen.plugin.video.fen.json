{
    "name"              : "FUCK",
    "plugin"            : "plugin.video.fen",
    "priority"          : 100,
    "assert"            : {
                            "play_movie":       ["title", "year"],
                            "play_episode":     ["showname", "season", "episode"],
                            "search_movie":     ["title"],
                            "search_episode":   ["showname"]
                          },
    "play_movie"        : [
                            "plugin://plugin.video.fen/?mode=get_search_term&media_type=movie{title_url}",
                            {"title": "(?i)^(\\[.*\\])?.*{title}.*(\\[.*\\])?$", "year": "{year}"}
                          ],
    "play_episode"      : [
                            "plugin://plugin.video.fen/?mode=get_search_term&media_type=tv_show{showname_url}",
                            {"title": "(?i)^(\\[.*\\])?{showname}(\\[.*\\])?$"},
                            {"return": "true", "season": "^{season}$", "episode": "^{episode}$"},
                            {"season": "^{season}$"},
                            {"season": "^{season}$", "episode": "^{episode}$"}
                          ],
    "search_movie"      : [
                            "plugin://plugin.video.fen/?mode=get_search_term&media_type=movie{title_url}",
                            {"keyboard": "{title}"}
                          ],
    "search_episode"    : [
                            "plugin://plugin.video.fen/?mode=get_search_term&media_type=tv_show{showname_url}",
                            {"keyboard": "{showname}"}
                          ]
}