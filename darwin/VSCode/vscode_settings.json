{
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "IronGeek.vscode-env"
  },
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  },
  "editor.wordBasedSuggestionsMode": "allDocuments",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never"
  },
  "editor.fontFamily": "'Cartograph CF', Menlo, Monaco, 'Courier New', monospace",
  "editor.formatOnSave": true,
  "editor.wordWrap": "on",
  "eslint.format.enable": true,
  "evenBetterToml.formatter.alignComments": true,
  "evenBetterToml.formatter.alignEntries": true,
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "evenBetterToml.formatter.arrayAutoExpand": true,
  "evenBetterToml.formatter.arrayTrailingComma": true,
  "evenBetterToml.formatter.columnWidth": 120,
  "evenBetterToml.formatter.indentEntries": true,
  "evenBetterToml.formatter.indentString": "  ",
  "evenBetterToml.formatter.indentTables": true,
  "evenBetterToml.formatter.inlineTableExpand": true,
  "evenBetterToml.formatter.reorderArrays": true,
  "evenBetterToml.formatter.reorderKeys": true,
  "files.associations": {
    "*.env*": "env",
    "*.env.local": "env",
    "*.env.production": "env",
    "*.scss": "scss",
    "*.tmpl": "shellscript",
    ".env": "env",
    ".env.base": "env",
    ".env*": "dotenv"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.defaultBranchName": "trunk",
  "git.openRepositoryInParentFolders": "never",
  "gitlens.plusFeatures.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.extension.toc.levels": "2..6",
  "material-icon-theme.files.associations": {
    "Caddyfile.base": "caddy",
    "*.env.base": "tune",
    "*.env.dist": "tune",
  },
  "projectManager.tags": [
    "Nos Futurs - Clients",
    "Nos Futurs - Internal",
    "Nos Futurs - Templates & Libraries",
    "Personal",
  ],
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "sortJSON.orderOverride": [
    "name",
    "version",
    "description",
    "license",
    "command",
    "when"
  ],
  "sortJSON.orderUnderride": [
    "dependencies",
    "devDependencies",
    "peerDependencies"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "DEBUG",
    "FIXME",
    "HACK",
    "INFO",
    "TODO",
    "XXX"
  ],
  "todohighlight.keywords": [
    {
      "backgroundColor": "rgba(0,0,0,0)", // INFO: setting the last parameter to zero (alpha channel) disables the background colour
      "border": "none",
      "color": "green",
      "isWholeLine": false,
      "text": "INFO:" // without a defined regex pattern this is the string that will be matched
    },
    {
      "after": {
        "contentText": "üößÔ∏è" // adds text after the highlight
      },
      "backgroundColor": "rgba(0,0,0,.2)",
      "before": {
        "contentText": "üößÔ∏è" // adds text before the highlight
      },
      "border": "1px solid orange",
      "borderRadius": "2px", // NOTE: use borderRadius along with `border` or you will see nothing change
      "color": "orange",
      "diagnosticSeverity": "information", // Set diagnostic severity to `none`, `information`, `warning` or `error`
      "isWholeLine": true,
      "text": "DEBUG:"
    },
    {
      "after": {
        "contentText": "üö®Ô∏è" // adds text after the highlight
      },
      "backgroundColor": "rgba(0,0,0,.2)",
      "before": {
        "contentText": "üö®Ô∏è" // adds text before the highlight
      },
      "border": "1px solid red",
      "borderRadius": "2px", // NOTE: use borderRadius along with `border` or you will see nothing change
      "color": "red",
      "diagnosticSeverity": "warning", // Set diagnostic severity to `none`, `information`, `warning` or `error`
      "isWholeLine": true,
      "text": "WARNING:"
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.handleKeys": {
    "<C-b>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    // FIX https://github.com/VSCodeVim/Vim/issues/8157#issuecomment-1426704638
    // Wait for new release, else we should migrate to using vscode-neovim extension
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    },
    {
      "before": [
        "c-r"
      ],
      "commands": [
        "redo"
      ]
    }
  ],
  "vim.smartRelativeLine": true,
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Bearded Theme Solarized Light",
  "workbench.iconTheme": "material-icon-theme",
  "autoflake-extension": {
    "expand-star-imports": true,
    "remove-duplicate-keys": true,
    "remove-unused-variables": true,
  },
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path-mac": "/Users/camille/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path-mac": "/Users/camille/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.formatOnType": false,
    "editor.formatOnSave": false,
  },
  "[javascript][typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
}